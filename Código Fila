#CÃ³digo de fila visto en clase 
class fila:
    def __init__(self):
        self.fila=[]
    def obtr(self):
        return self.fila.pop()
    def meter(self,e):
        self.fila.append(e)
        return len(self.fila)
    @property
    def lon(self):
        return len(self.fila)


f=fila()
f.meter(1)
f.meter(2)
f.meter('a')
f.meter("b")
print(f.lon)
print(f.obtr())
print(f.obtr())
print(f.obtr())
print(f.obtr())
print(f.lon)

